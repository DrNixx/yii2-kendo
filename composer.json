{
    "name": "drnixx/yii2-kendo",
    "description": "PHP wrappers for KendoUI components",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.6",

        "drnixx/yii2-onix": "^1.5.18",
        "drnixx/yii2-jquery-migrate-asset": "^3.0.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Panteleeff",
            "email": "drnix@chess-online.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "kendo\\": "src/"
        }
    },
    "config": {
        "platform": {"php": "5.6"}
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
